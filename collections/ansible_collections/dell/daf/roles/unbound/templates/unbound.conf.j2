{% if unbound_zones is defined %}
{%   for item in unbound_zones %}
{%     for item2 in item['a_records'] %}
local-data: "{{ item2['name'] }}.{{ item['zone'] }}. IN A {{ item2['ip'] }}"
{%     endfor %}
{%     for item2 in item['a_records'] %}
local-data-ptr: "{{ item2['ip'] }} {{ item2['name'] }}.{{ item['zone'] }}"
{%     endfor %}
{%   endfor %}
{% endif %}
